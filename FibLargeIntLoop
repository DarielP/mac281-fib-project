#include<iostream>
#include<ctime>
#include "InfInt.h"//allows int to store large integers
using namespace std;
int main(){
clock_t start = clock();
//N=100, 500, 1000
int n = 100;
InfInt first = "0", second = "1", sum;

for(int i = 2; i <= n; i++)
{ 
sum = first+second;
first = second;
second = sum; 
}
 
clock_t end = clock();
double duration = (double)(end-start)/CLOCKS_PER_SEC;
cout << "Fibonacci at: " << n << " is: " << sum << endl; 
cout << "Time taken: " << duration << " seconds." << endl;
return 0; }

